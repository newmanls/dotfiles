super + space
    ~/.local/bin/dmenu-run-i

super + shift + space
    ~/.config/rofi/scripts/appsmenu.sh

super + Return
    st

super + shift + q
    xdotool getwindowfocus windowkill

super + e
    st -e lfub

super + p
    st -e cmus

super + w
    firefox

super + c
    qalculate-gtk

super + l
    ~/.local/bin/lockscreen

super + shift + {equal,minus}
    ~/.local/bin/notify-backlight {up,down}

super + {equal,minus,0}
    ~/.local/bin/notify-pulse {up,down,mute}

super + shift + s
    ~/.local/bin/dmenu-screenshot

super + shift + Escape
    ~/.local/bin/dmenu-session

super + alt + space
    ~/.local/bin/emojipick

super + alt + {m,u}
    ~/.local/bin/dmenu-disks {mount,unmount}

super + alt + n
    ~/.local/bin/dmenu-nm
