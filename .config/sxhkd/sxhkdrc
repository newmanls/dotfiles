super + {_, shift + } space
    {dmenu-run-i, ~/.config/rofi/scripts/appsmenu.sh}

super + shift + q
    xdotool getwindowfocus windowkill

super + {Return, e, n, p}
    st -e {_, lfub, newsboat, cmus}

super + {w, c, l}
    {firefox, qalculate-gtk, lockscreen}

super + {0, minus, equal}
    notify-pulse {mute, down, up}

super + shift + {minus, equal}
    notify-backlight {down, up}

super + shift + {Escape, s}
    dmenu-{session,screenshot}

super + alt + {space, m, u, n}
    dmenu-{emoji,disks mount,disks unmount,nm}
