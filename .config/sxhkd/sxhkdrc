super + space
    ~/.local/bin/dmenu_run_i

super + shift + space
    ~/.config/rofi/scripts/appsmenu.sh

super + Return
    st

super + shift + q
    xdotool getwindowfocus windowkill

super + e
    st -e lfub

super + p
    st -e cmus

super + w
    firefox

super + c
    qalculate-gtk

super + shift + {equal,minus}
    ~/.local/bin/notify-backlight {up,down}

super + {equal,minus,0}
    ~/.local/bin/notify-pamixer {up,down,mute}

super + shift + s
    ~/.local/bin/dmenu_screenshot

super + shift + Escape
    ~/.local/bin/dmenu_session

super + alt + space
    ~/.local/bin/emojipick

super + alt + {m,u}
    ~/.local/bin/dmenu_disks {mount,unmount}

super + alt + g
    ~/.local/bin/dmenu_emu

super + alt + n
    ~/.local/bin/dmenu_nm
